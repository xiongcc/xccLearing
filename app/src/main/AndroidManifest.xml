        <activity android:name=".xcc.ancitvity.RankViewActivity">
        </activity>
        <activity android:name=".xcc.ancitvity.TestActivity">
        </activity>
        <activity android:name=".xcc.ancitvity.AutoLActivity">
        </activity>

